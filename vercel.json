{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front End/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(assets|favicon.ico|icon-192.png)/.*",
      "dest": "/Front End/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Front End/dist/index.html"
    }
  ]
}
